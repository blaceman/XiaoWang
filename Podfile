platform :ios, '8.0'
inhibit_all_warnings!
target 'XiaoWang' do
    pod 'AFNetworking'
    pod 'SDWebImage' , '< 4.3.0'#此处限制版本是因为PYPhotoBrowser不支持高版本的SD，后续支持的话可不做
    pod 'Masonry'
    pod 'MJRefresh'
    pod 'MJExtension'
    pod 'YYKit'
    pod 'JKCategories' #iOS类别库
    pod 'EasyNavigation' #自定义导航栏
    pod 'TZImagePickerController'#仿微信图片选择器
    pod 'PYPhotoBrowser'#图片浏览器
    pod 'MBProgressHUD'
    pod 'UITableView+FDTemplateLayoutCell'
    pod 'WMPageController'
    pod 'SDCycleScrollView' #banner库
    pod 'TPKeyboardAvoiding'#防止键盘遮挡
    pod 'ReactiveObjC'
    pod 'XXNibBridge', '~> 2.3.0'
    pod 'JPush'
    
    pod 'AliyunOSSiOS'
    pod 'UMengAnalytics-NO-IDFA'
    pod 'UMengUShare/Social/WeChat', '< 6.4.7'
    pod 'UMengUShare/Social/QQ', '< 6.4.7'
    pod 'UMengUShare/Social/Sina', '< 6.4.7'
    pod 'BRPickerView'
    
    pod 'NIMSDK' #网易云信SDK
    pod 'NIMKit/Full' #网易云信 UI完整版
    
end

